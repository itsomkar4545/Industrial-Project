<div class="main-body">
    <div class="table-section">
        <table id="users-table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <td>{{this.id}}</td>
                    <td>{{this.name}}</td>
                    <td>{{this.email}}</td>
                    <td><button class="btn-danger">Delete</button></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    
</div>